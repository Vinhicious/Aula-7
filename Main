public class Main {
    public static void main(String[] args){
        Carro carro = new Carro("Chevrolet", "Onix", 2010, 4);
        Moto moto = new Moto("Harley Davidson", "Low Rider", 2023, true);
        
        
        moto.Moto();
        carro.Carro();
        moto.Empinar();
        carro.Acelerar();
    }
}

